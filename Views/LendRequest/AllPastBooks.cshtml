
@model IEnumerable<LibraryManagmentSystem.Models.LendRequest>
 <div class="container">
  <div class="row justify-content-md-center"> 
<div class="col-md-12">
    <div class="card">
        <div class="card-body">
                  <div class="row">
                     <div class="col">
                        <center>
                           <h4> Returned and Declined Book List </h4>
                        </center>
                     </div>
                  </div>
                  <div class="row">
                     <div class="col">
                        <center>
                           <img width="100px" src="~/Images/imgs/books.png" />
                        </center>
                     </div>
                  </div>
                        <div class="row">
                     <div class="col">
                        <hr>
                     </div>
                  </div>
                  <div class="row">
                     <div class="col">
                        <table class="table table-striped table-bordered" >
                            <thead>
                                <tr>
                                <td>Lend Id </td>
                                <td>User Name</td>
                                <td>Book Name</td>
                                <td>Due date</td>
                                <td>Return Date</td>
                                <td>Fine Amount</td>
                                <!--<td>Status</td>-->
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var list in Model)
                                {
                                    <tr>
                                        <td>@list.LendId</td>
                                        <td>@list.AccountsInfo.UserName</td>
                                        <td>@list.BooksInfo.BookTitle</td>
                                        <td>@list.LendDate.AddDays(15)</td>
                                        <td>@list.ReturnDate</td>
                                        <td>@list.FineAmount</td>
                                        <!--
                                            
                                        </td>-->
                                     </tr>
                                }  
                        </tbody>
                        </table>
                     </div>
                  </div>
        </div>
    </div>
</div>
</div>
</div>